@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="~/assets/img/AutraLogo/8.png">
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/my-icons-collection/font/flaticon.css">
    <link href="~/css/sidebar.css" rel="stylesheet" />
    <link href="~/css/modal.css" rel="stylesheet" />
    <style>
        @@keyframes blink {
            0% {
                opacity: 1;
            }

            50% {
                opacity: 0.15;
            }

            100% {
                opacity: 1;
            }
        }

        .img {
            animation: blink 2s;
            animation-iteration-count: infinite;
        }
    </style>
    <title>Editor</title>
</head>

<body>
    <input type="text" hidden disabled id="projectName" value='@ViewData["projectName"]' />
    <canvas id="canvas" style="position:absolute"></canvas>

    <!--Start-up window-->
    <div class="modelDivSize" id="modalDivDetails" style="display:none">
        <a asp-area="" asp-controller="Home" asp-action="Index" class="modalCloseBtn " id="closeStartUpBtn"><i
                class="fas fa-times"></i></a>
        <div class="container mt-3">
            <div class="row">
                <div class="col-8" style=" padding-left: 0px!important;">
                    <h1></h1>
                </div>
                <div class="col-4" style="align-content:end;">
                    <a asp-area="" asp-controller="Home" asp-action="Index" style=" color: inherit;">

                        <img src="/assets/img/AutraLogo/7.png" alt="AUTRA"
                            style="width: 100%;padding-left: 150px; padding-right: 5px; padding-top: 5px;" />
                    </a>
                </div>
            </div>

            <div class="col" id="modalPage2" style=" padding-left: 25px!important;">
                <div class="row">
                    <form action="#" id="form2StructureData" name="form2StructureData">
                        <table class="myTable">
                            <tr class="form-group m-4" style="margin-top:20px;">
                                <td>
                                    <h1 class="fontBlue" id="modalNewProjectTitle" style="padding-bottom: 10px;"> New Truss Project</h1>
                                </td>
                            </tr>
                            <tr class="form-group mt-4" style="margin-top:20px;">
                                <td>
                                    <h3>Truss Layout</h3>
                                </td>
                            </tr>
                        </table>
                        <table class="myTable">
                            <tr class="form-group">
                                <td>
                                    <h6 class="subtitle" style=" color: #04DBEC;">Grids</h6>
                                </td>
                            </tr>
                            <tr class="form-group">
                                <td><label for="spaceX">Spacing in X direction</label></td>
                                <td> <input type="text" name="spaceX" id="spaceX" class="myModalInput" placeholder="Ex:3*6 or 4.5 6 8" required></td>
                            </tr>
                            <tr class="form-group">
                                <td><label for="spaceZ">Spacing in Y direction</label></td>
                                <td> <input type="text" name="spaceZ" id="spaceZ" class="myModalInput" placeholder="Ex:3*6 or 4.5 6 8" required></td>
                            </tr>
                            <tr class="form-group">
                                <td><label for="height">Spacing in Z direction</label></td>
                                <td><input type="text" name="spaceY" id="spaceY" class="myModalInput" placeholder="Ex:3*6 or 4.5 6 8" required></td>
                            </tr>
                            <tr style="display:none;">
                                <td>
                                    <h6 class="subtitle" style=" color: #04DBEC;">Drawing Mode </h6>
                                </td>
                                <td>
                                    <input class="radio form-group" type="radio" name="modegrp" id="autoMode" value="autoMode" checked>Automated
                                    &nbsp; &nbsp; <input class="radio form-group" type="radio" name="modegrp" id="manMode" value="manMode">Manual
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <h6 class="mt-4" for="material" style=" color: #04DBEC;">Material </h6>
                                </td>
                            </tr>
                            <tr class="form-group">
                                <td><label for="modulus">Modulus of Elasticity (E)</label></td>
                                <td> <input type="text" name="modulus" id="modulus" class="myModalInput" placeholder="Ex:2e3 or 2000" required></td>
                            </tr>
                            <tr class="form-group">
                                <td>
                                    <h6 class="subtitle mt-4" style=" color: #04DBEC;">Drawing Mode</h6>
                                </td>
                            </tr>
                            <tr class="form-group">
                                <td> <label for="mainOrient">Elements to draw </label></td>
                                <td>
                                    @* <input class="radio form-group" type="radio" name="optradio" id="nes" value="NE" checked>Elements and Nodes &nbsp; &nbsp; *@
                                    <input class="radio form-group" type="radio" name="optradio" id="ns" value="N" checked>Nodes &nbsp; &nbsp;
                                    <input class="radio form-group" type="radio" name="optradio" id="none" value="E">None
                                </td>
                            </tr>
                            <tr class="form-group">
                                <td><label for="crossA">Area (A)</label></td>
                                <td> <input type="text" name="crossA" id="crossA" class="myModalInput" required></td>
                            </tr>
                        </table>
                    </form>
                </div>
                <div class="row">
                    <form action="#" id="form3LoadData" name="form3LoadData">
                        <a id="modalFlipBackward" class="btn btn-secondary" style="margin-left: 340px;">Back</a>
                        <button type="button" class="btn btn-primary" style="background-color: #04DBEC !important; color: #000D20 !important;" id="createGrids">
                            Create Truss
                        </button>
                    </form>
                </div>
            </div>

            </div>
        </div>
    </div>

    <input type="file" id="upload" accept="text/aut" hidden />
    <div class="btn" id="sidebarBtn">
        <span class="fas fa-bars"></span>
    </div>
    <nav class="sidebar">
        <div class="List">
            <div class="taskbarIcon">
                <section>

                    <div class="taskbarIconBlock">
                        <div class=" icon">
                            <div class="taskbarIconLabel"><img src="/assets/img/AutraLogo/8.png" id="autraLogo"
                                    alt="AUTRA" style="width:40%;padding-top:8px;" /></div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" id="measureIcon">
                        <div class="flaticon-ruler icon">
                            <div class="taskbarIconLabel">Measure</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" onclick="hideDimensions()" id="hideDimensions">
                        <div class="flaticon-dimension icon">
                            <div class="taskbarIconLabel">Hide Dimensions</div>
                        </div>
                    </div>
                </section>

                <section>
                    <div class="taskbarIconBlock" onclick="showDimensions()" id="showDimensions" style="display:none">
                        <div class="flaticon-dimension icon">
                            <div class="taskbarIconLabel">Show Dimensions</div>
                        </div>
                    </div>
                </section>


                <section>
                    <div class="taskbarIconBlock" onclick="screenshot()">
                        <div class="flaticon-3d-modeling icon">
                            <div class="taskbarIconLabel">Screenshot</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" id="viewsIcon">
                        <div class="flaticon-axis-2 icon">
                            <div class="taskbarIconLabel">Views</div>
                        </div>
                    </div>
                </section>
                <section>

                    <div class="taskbarIconBlock" id="dark" onclick="darkTheme()">
                        <div class="flaticon-moon icon">
                            <div class="taskbarIconLabel">Dark Mode</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" id="light" onclick="lightTheme()" style="display: none;">
                        <div class="flaticon-sun icon">
                            <div class="taskbarIconLabel">Light Mode</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" onclick="toggle()">
                        <div class="flaticon-beam-1 icon">
                            <div class="taskbarIconLabel">Toggle view</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" id="addElement">
                        <div class="flaticon-add-1 icon">
                            <br>
                            <div class="taskbarIconLabel">Add Element</div>
                        </div>
                    </div>
                    <section class="ddList">
                        <ul>
                            <li class="ddListItem" id="addNodeCoord">Draw node</li>
                            <li class="ddListItem" id="drawElement"> Draw Element </li>
                            @* <li class="ddListItem" id="addNodeBeam"> Add node on beam</li> *@
                        </ul>
                    </section>
                </section>
                <section>
                    <div class="taskbarIconBlock" id="edit">
                        <div class="flaticon-cube icon">
                            <div class="taskbarIconLabel">Edit</div>
                        </div>
                    </div>
                    <section class="ddList">
                        <ul>
                            <li class="ddListItem" id="elementSection"> Element Section </li>
                            <li class="ddListItem" id="moveSection">Move Element </li>
                            <li class="ddListItem" id="copySection">Copy Element</li>
                        </ul>
                    </section>
                </section>
                <section>
                    <div class="taskbarIconBlock">
                        <div class="flaticon-business icon">
                            <div class="taskbarIconLabel">Forces</div>
                        </div>
                    </div>
                    <section class="ddList">
                        <ul>
                            @*<li class="ddListItem" id="floorLoadIcon">Add Floor load </li>*@
                            @* <li class="ddListItem" id="lineLoadIcon"> Add Line load </li> *@
                            <li class="ddListItem" id="pointLoadIcon"> Add Nodal Force</li>
                            <li class="ddListItem" id="showLoadIcon" onclick="showLoads()"> Show Forces</li>
                            <li class="ddListItem" id="hideloadIcon" onclick="hideLoads()"> Hide Forces</li>
                        </ul>
                    </section>
                </section>
                <section>
                    <div class="taskbarIconBlock" onclick="save()">
                        <div class="flaticon-floppy-disk icon">
                            <div class="taskbarIconLabel">Save</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock " onclick="solve()">
                        <div class="flaticon-play-button icon">
                            <div class="taskbarIconLabel">Run Model</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock " id="analysisResult" style="display:none;">
                        <div class="flaticon-graphs icon">
                            <div class="taskbarIconLabel">Analysis Results</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock " id="generateDrawings" style="display:none;"
                        onclick="generateDrawings()">
                        <div class="flaticon-floor-plan icon">
                            <div class="taskbarIconLabel">Drawings</div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock" onclick="downloadFile()">
                        <div class="flaticon-save icon">
                            <div class="taskbarIconLabel">Download </div>
                        </div>
                    </div>
                </section>
                <section>
                    <div class="taskbarIconBlock">
                        <a asp-area="" asp-controller="Home" asp-action="Index" style=" color: inherit;">
                            <div class="flaticon-browser icon">
                                <div class="taskbarIconLabel">Home</div>
                            </div>
                        </a>
                    </div>
                </section>
            </div>
        </div>
    </nav>
    <div class="optionsDetails">
        <div id="drawElementDetails" class="popupDiv large2DivSize" style="padding-top: 5px;">
            <div class="innerDiv" >
                <table>
                    <thead>
                        <th style="width: 100%; text-align: right;">
                            Draw Element
                        </th>
                    </thead>
                    <tbody>
                        <tr>
                            <th>Start Node Id</th>
                            <td><input type="text" id="startNodeNumber" size="5"></td>
                        </tr>
                        <tr>
                            <th>End Node Id</th>
                            <td><input type="text" id="endNodeNumber" size="5"></td>
                        </tr>
                        <tr>
                            <th>Cross Sec. Area (A)</th>
                            <td><input type="text" id="crossSectionalArea" size="5"></td>
                        </tr>
                        <tr>
                            <th>Young Modulus (E)</th>
                            <td><input type="text" id="modulusOfElasticity" size="5"></td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <div class="buttonContainer" style="text-align: center;">
                    <button class="myBtn" onclick="startDrawMode()">Draw</button>
                    <button class="myBtn" onclick="endDrawMode()">Close</button>
                </div>
            </div>
        </div>
        <div id="addNodeCoordDetails" class="popupDiv narrowShortDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn7" id="addNodeCoordBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <tr>
                            <th> Add Node </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th>X</th>
                            <td><input type="text" id="nodeXCoord" value="0" size="5">&nbsp;m </td>
                        </tr>
                        <tr>
                            <th>Y</th>
                            <td><input type="text" id="nodeZCoord" value="0" size="5">&nbsp;m</td>
                        </tr>
                        <tr>
                            <th>Z</th>
                            <td><input type="text" id="nodeYCoord" value="0" size="5">&nbsp;m</td>
                        </tr>
                    <tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="createNode()">Done</button>
            </div>
        </div>
        <div id="addNodeBeamDetails" class="popupDiv meduimDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn3" id="addNodeBeamBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <th> Add node </th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Distance from beam start</td>
                            <td><input type="text" id="nodeToBeam" value="0" size="5"></td>
                            <td>&nbsp;m</td>
                        </tr>
                    <tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="addNodeToBeam()">Done</button>
            </div>
        </div>

        <div id="elementSectionDetails" class="popupDiv narrow2DivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn5" id="elementSectionBtn"><i class="fas fa-times"></i></button>
                <table>
                    <tbody>
                        <tr>
                            <th>Change section to: &nbsp</th>
                            <td>
                                <select id="section" style="width:120%;">
                                    <option value="IPE80">IPE80</option>
                                    <option value="IPE100">IPE100</option>
                                    <option value="IPE120">IPE120</option>
                                    <option value="IPE140">IPE140</option>
                                    <option value="IPE160">IPE160</option>
                                    <option value="IPE180">IPE180</option>
                                    <option value="IPE200">IPE200</option>
                                    <option value="IPE220">IPE220</option>
                                    <option value="IPE240">IPE240</option>
                                    <option value="IPE270">IPE270</option>
                                    <option value="IPE300">IPE300</option>
                                    <option value="IPE330">IPE330</option>
                                    <option value="IPE360">IPE360</option>
                                    <option value="IPE400">IPE400</option>
                                    <option value="IPE450">IPE450</option>
                                    <option value="IPE500">IPE500</option>
                                    <option value="IPE550">IPE550</option>
                                    <option value="IPE600">IPE600</option>
                                </select>
                            </td>
                        </tr>
                    <tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="changeSection()">Done</button>

            </div>
        </div>
        <div id="moveSectionDetails" class="popupDiv narrowLongDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn4" id="moveSectionBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <tr>
                            <th> Move element </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>X</td>
                            <td><input type="text" id="xMove" value="0" size="5">&nbsp;m</td>
                        </tr>
                        <tr>
                            <td>Y</td>
                            <td><input type="text" id="zMove" value="0" size="5">&nbsp;m</td>
                        </tr>
                        <tr>
                            <td>Z</td>
                            <td><input type="text" id="yMove" value="0" size="5">&nbsp;m</td>
                        </tr>
                    <tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="move()">Done</button>
            </div>
        </div>
        <div id="copySectionDetails" class="popupDiv narrowLongDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn4" id="copySectionBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <tr>
                            <th> Copy element </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>X</td>
                            <td><input type="text" id="xCopy" value="0" size="5">&nbsp;m</td>
                        </tr>
                        <tr>
                            <td>Y</td>
                            <td><input type="text" id="zCopy" value="0" size="5">&nbsp;m</td>
                        </tr>
                        <tr>
                            <td>Z</td>
                            <td><input type="text" id="yCopy" value="0" size="5">&nbsp;m</td>
                        </tr>
                        <tr>
                            <td>No. of copies &nbsp; </td>
                            <td> <input type="text" id="Replication" value="1" size="5"></td>
                        </tr>
                    <tbody>
                </table>
                <br style="line-height:10px;">
                <button type="button" class="myBtn" onclick="copy()">Done</button>
            </div>
        </div>

        <div id="floorLoadDetails" class="popupDiv meduimDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn3" id="floorLoadBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <tr>
                            <th>Add floor Load</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Load Magnitude &nbsp;</td>
                            <td><input type="text" id="floorLoad" size="5"></td>
                        </tr>
                        <tr>
                            <td>Load pattern &nbsp;</td>
                            <td>
                                <select id="floorLoadCase" style="width:100%;">
                                    <option value="dead">Dead</option>
                                    <option value="live">Live</option>
                                </select>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="addFloorLoad()">Done</button>
            </div>
        </div>
        <div id="lineLoadDetails" class="popupDiv large2DivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn22" id="lineLoadBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <tr>
                            <th>Add Line Load</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Load Magnitude &nbsp;</td>
                            <td><input type="text" id="lineLoad" size="5">&nbsp;</td>
                            <td> ton/m </td>
                        </tr>
                        <tr>
                            <td>Load pattern &nbsp;</td>
                            <td>
                                <select id="lineLoadCase" style="width:95%;">
                                    <option value="dead">Dead</option>
                                    <option value="live">Live</option>
                                </select>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="addLineLoad()">Done</button>
            </div>
        </div>
        <div id="pointLoadDetails" class="popupDiv large2DivSize" style="padding-top: 5px;">
            <div class="innerDiv">
                <table>
                    <thead>
                        <tr>
                            <th style="text-align: right; width: 100%;">Add Nodal Force</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Node Id &nbsp;</td>
                            <td><input type="text" id="loadNodeId" size="5"></td>
                        <tr>
                            <td>Fx &nbsp;</td>
                            <td><input type="text" id="fx" size="5"></td>
                        </tr>
                        <tr>
                            <td>Fy &nbsp;</td>
                            <td><input type="text" id="fy" size="5"></td>
                        </tr>
                        <tr>
                            <td>Fz &nbsp;</td>
                            <td><input type="text" id="fz" size="5"></td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <div class="buttonContainer" style="text-align: center;">
                    <button class="myBtn" onclick="addPointLoad()">Add</button>
                    <button class="myBtn" onclick="endPointLoad()">Close</button>
                </div>
            </div>
        </div>
        @* <div id="showLoadDetails" class="popupDiv meduimDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn3" id="showLoadBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <th>
                            Show Loads &nbsp;&nbsp;
                        </th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Load pattern </td>
                            <td>
                                <select id="showLoadCase">
                                    <option value="dead">Dead</option>
                                    <option value="live">Live</option>
                                </select>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <button type="button" class="myBtn" onclick="showLoads()">Done</button>

            </div>
        </div> *@

        <div id="analysisResultDetails" class="popupDiv largeDivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn2" id="analysisResultBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <th>
                            Analysis results
                        </th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Load Pattern</td>
                            <td>
                                <select id="resultPattern" style="width:100%;">
                                    <option value="dead">Dead</option>
                                    <option value="live">Live</option>
                                    <option value="combination">combination</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>Straining Action &nbsp;</td>
                            <td>
                                <select id="strainingAction" style="width:100%;">
                                    <option value="Mo">Moment</option>
                                    <option value="V">Shear</option>
                                    <option value="No">Normal</option>
                                    <option value="rv">Reaction</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td> Display Mode &nbsp;</td>
                            <td>
                                <select id="display" style="width:100%;">
                                    <option value="0">Wireframe</option>
                                    <option value="1">Fill Diagrams</option>
                                </select>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <button class="myBtn" onclick="result()">Show Result</button>
                <button class="myBtn" onclick="hideResults()">Hide Result</button>
                <br>
                <br>

                <table style="display:table; width:100%;">
                    <tr>
                        <td>
                            <label>Station </label>

                        </td>
                        <td>
                            <input type="text" id="station" class="myModalInput" disabled size="5"> <br />

                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>Action</label>

                        </td>
                        <td>
                            <input type="text" id="action" class="myModalInput">

                        </td>
                    </tr>
                </table>
            </div>
        </div>

        <div id="viewsDetails" class="popupDiv large3DivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn22" id="viewsBtn"><i class="fas fa-times"></i></button>
                <table>
                    <thead>
                        <th>
                            Rendered Views &nbsp;
                        </th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>View</td>
                            <td>
                                <select id="view" onchange="changeView()" style="width:110%;">
                                    <option>Select view</option>
                                    <option value="top">Top</option>
                                    <option value="front">Front</option>
                                    <option value="back">Back</option>
                                    <option value="right">Right</option>
                                    <option value="left">Left</option>
                                    <option value="3D">3D</option>
                                </select>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div id="measureDetails" class="popupDiv small2DivSize">
            <div class="innerDiv">
                <button class="popupBtn myBtn6" id="measureBtn"><i class="fas fa-times"></i></button>
                <table>
                    <tbody>
                        <tr>
                            <th>
                                Distance &nbsp;
                            </th>
                            <td>
                                <input type="text" id="distance" disabled style="color:black" size="5">
                            </td>
                        </tr>
                    </tbody>
                </table>
                <br>
                <button class="myBtn" onclick="measure()">Measure</button>
            </div>
        </div>
    </div>

    <div id="elementData" style="position:absolute;bottom:20px;right:20px;display: none">
        Element Data :
        ID <input type="text" id="beamId" disabled size="3" style="margin-left:5px;padding-left:5px">
        StartPosition<input type="text" id="beamStart" disabled size="7" style="margin-left:5px;padding-left:5px">
        EndPosition<input type="text" id="beamEnd" disabled size="7" style="margin-left:5px;padding-left:5px">
        A <input type="text" id="area" disabled size="7" style="margin-left:5px;padding-left:5px">
        E <input type="text" id="modulusE" disabled size="7" style="margin-left:5px;padding-left:5px">
        @* DeadLoad<input type="text" id="beamDead" disabled size="7" style="margin-left:5px;padding-left:5px"> *@
        @* LiveLoad<input type="text" id="beamLive" disabled size="7" style="margin-left:5px;padding-left:5px"> *@
    </div>
    <div id="nodeData" style="position:absolute;bottom:20px;right:20px">
        Node Data :
        ID <input type="text" id="nodeId" disabled size="3" style="margin-left:5px;padding-left:5px">
        Position <input type="text" id="nodePosition" disabled size="7" style="margin-left:5px;padding-left:5px">
        Load <input type="text" id="nodeLoad" disabled size="7" style="margin-left:5px;padding-left:5px">
        Constraint <input type="text" id="nodeConstraint" disabled size="7" style="margin-left:5px;padding-left:5px">
    </div>


    <!-- Notifaiction Modal -->
    <div class="modal" id="infoModal" tabindex="-1" role="dialog" data-backdrop="static"
        aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-sm" role="document">
            <div class="modal-content" style="background-color:#000D20">
                <div class="modal-body" id="info" style="color:white">
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-primary" data-dismiss="modal">Ok</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Loading Modal -->
    <div class="modal" id="staticBackdrop" data-backdrop="static" tabindex="-1" role="dialog"
        aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content" style="background-color:#000D20">
                <div class="modal-header">
                    <h5 class="modal-title" id="staticBackdropLabel" style="color:white">Processing ....</h5>
                </div>
                <div class="modal-body">
                    <div class="spinner-grow text-primary" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-secondary" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-success" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-danger" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-warning" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-info" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-light" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                    <div class="spinner-grow text-dark" role="status" style="display:inline-block">
                        <span class="sr-only">Loading...</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Closing Modal -->
    <div class="modal" id="closingModal" data-backdrop="static" tabindex="-1" role="dialog"
        aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content" style="background-color:#000D20">
                <div class="modal-body" style="color:white">
                    Do you want to save the changes made to the model?
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" id="save">Save</button>
                    <button type="button" class="btn btn-danger" id="dontSave">Don't Save</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
    </div>

